{
  "expo": {
    "name": "Simple Fridge Organizer",
    "slug": "simple-fridge-organizer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jonathanaguayo.simple-fridge-organizer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Simple application that allows users to keep track of their groceries by scanning the barcode and adding the item to their virtual fridge. ",
    "githubUrl": "https://github.com/Jonathan-Aguayo/Fridge-organizer"
  }
}
